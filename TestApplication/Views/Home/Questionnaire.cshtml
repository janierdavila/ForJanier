@using TestApplication.Models
@model  Questionnaire

@using (Html.BeginForm("Questionnaire", "Home", FormMethod.Post))
{
    <div>
        @for(int i = 0; i < Model.Questions.Count; i++)
        {
            @Html.EditorFor(m => m.Questions[i]);
        }
    </div>
    <div>
        <button type="submit">Save</button>
    </div>
}